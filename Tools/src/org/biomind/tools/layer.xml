<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="File">
            <file name="org-biomind-tools-ToolsAction.instance">
                <attr name="delegate" newvalue="org.biomind.tools.ToolsAction"/>
                <attr name="displayName" bundlevalue="org.biomind.tools.Bundle#CTL_ToolsAction"/>
                <attr name="iconBase" stringvalue="org/biomind/tools/xfce-system.png"/>
               
                <attr name="noIconInMenu" boolvalue="false"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="File">
            <file name="org-biomind-tools-ToolsAction.shadow">
                <attr name="originalFile" stringvalue="Actions/File/org-biomind-tools-ToolsAction.instance"/>
                <attr name="position" intvalue="300"/>
            </file>
        </folder>
    </folder>
</filesystem>
